{
  "name": "dari-dictionary-api",
  "version": "1.0.0",
  "description": "Dari Dictionary API built with express.js for the Android App.",
  "main": "dist",
  "scripts": {
    "start": "nodemon --ignore src/assets -w src --exec \"babel-node src --presets es2015\"",
    "build": "babel src -D -d dist --presets es2015",
    "testRoutes": "nodemon -w test-routes --exec \"babel-node test-routes --presets es2015\"",
    "test": "eslint src",
    "watch": "webpack --display-error-details --config webpack.config.js --watch",
    "prettier": "prettier --write src/**/*.js",
    "prettier-check": "prettier --config .prettierrc.js src/**/*.js -l"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rotexhawk/DariDictionary.git"
  },
  "author": "Yasin Yaqoobi",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "bootstrap-sass": "^3.3.7",
    "command-line-args": "^4.0.1",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "express-mailer": "^0.3.1",
    "express-winston": "^2.0.0",
    "helmet": "^3.3.0",
    "isemail": "^2.2.1",
    "jquery": "^3.1.1",
    "jquery-ui": "^1.12.1",
    "mailgun-js": "^0.8.0",
    "moment": "^2.17.1",
    "nunjucks": "^3.0.0",
    "path": "^0.12.7",
    "request-ip": "^1.2.3",
    "sqlite3": "^3.1.8",
    "winston": "^2.3.0",
    "wordpos": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^4.0.0",
    "nodemon": "^1.9.2",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.0.2",
    "shipit-cli": "^2.0.0",
    "shipit-deploy": "^2.4.0",
    "sitemap": "^1.9.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0"
  }
}
